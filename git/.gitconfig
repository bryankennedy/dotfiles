
[core]
	excludesfile = ~/.gitignore_global
[diff]
	push = matching
[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "status"]
	changed = green
	untracked = cyan
	added = yellow
[color "diff"]
	new = green bold
	frag = magenta bold
	meta = yellow bold
	old = red bold
[init]
	defaultBranch = main
[user]
	name = bkennedy
	email = bkennedy@smm.org
